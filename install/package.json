{
  "name": "qlty-action-install",
  "version": "0.0.0",
  "homepage": "https://github.com/qltysh/qlty-action",
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "package": "esbuild src/index.ts --bundle --platform=node --target=es2020 --outfile=dist/index.js",
    "all": "npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@sentry/node": "^9.12.0",
    "esbuild": "^0.25.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "typescript": "^5.8.3"
  }
}
